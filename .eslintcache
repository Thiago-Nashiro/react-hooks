nst [number, setNumber] = useState(1)\n    const [fatorial, setFatorial] = useState(1)\n    \n    useEffect(function(){\n        setFatorial(calcFatoria(number))\n    }, [number])\n\n    const [status, setStatus] =useState(\"Impar\")\n    useEffect(function(){\n        setStatus(number % 2 == 0 ? \"Par\" : \"Ímpar\")\n    }, [number])\n\n\n    return (\n        <div className=\"UseEffect\">\n            <PageTitle\n                title=\"Hook UseEffect\"\n                subtitle=\"Permite executar efeitos colaterais em componentes funcionais!\"\n            />\n            <SectionTitle title=\"exercicio #1\" />\n             <div className=\"center\">\n\n                 <div className=\"center\">\n                 <span className=\"text\">Fatorial</span>\n                 <span className=\"text red\">{fatorial}</span>\n                 </div>\n                    <input type=\"number\" className=\"input\" value={number} onChange={e=> setNumber(e.target.value) } />\n             </div>\n             <SectionTitle title=\"exercicio #2\" />\n             <div className=\"center\">\n                 \n                 <div>\n                 <span className=\"text\">Status:</span>\n                \n\n                 </div>\n                 <span className=\"text red\">{status} </span>\n                 \n             </div>\n        </div>\n\n           \n    )\n}\n\nexport default UseEffect\n","C:\\Users\\thiag\\OneDrive\\Área de Trabalho\\cursos\\react\\hooks\\src\\views\\examples\\UseMemo.jsx",["94"],"import React, { useEffect, useMemo, useState } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\n\nconst UseMemo = (props) => {\n\n    function soma (a,b){\n        const future = Date.now()+ 100\n        while(Date.now() < future ){} \n        return a+b\n    }\n\n\n    const [ n1, setN1 ] = useState(0)\n    const [ n2, setN2 ] = useState(0)\n    const [ n3, setN3 ] = useState(0)\n\n    const result =useMemo(() => soma(n1, n2), [n1,n2])\n\n\n    return (\n        <div className=\"UseMemo\">\n            <PageTitle\n                title=\"Hook UseMemo\"\n                subtitle=\"Retorna um valor memoizado!\"\n            />\n\n\n            <div className=\"center\">\n                <input type=\"number\" className=\"input\" value={n1}\n                onChange = {e =>setN1(parseInt(e.target.value))}\n                />\n                <input type=\"number\" className=\"input\" value={n2}\n                onChange = {e =>setN2(parseInt(e.target.value))}\n                />\n                <input type=\"number\" className=\"input\" value={n3}\n                onChange = {e =>setN3(parseInt(e.target.value))}\n                />\n                <span className=\"text\">{result}</span>\n            </div>\n        </div>\n\n    )\n}\n\nexport default UseMemo\n","C:\\Users\\thiag\\OneDrive\\Área de Trabalho\\cursos\\react\\hooks\\src\\views\\examples\\UseContext.jsx",["95","96","97"],"import React, { useContext, useEffect } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\nimport SectionTitle from '../../components/layout/SectionTitle'\nimport DataContent from '../../data/DataContext'\nimport { AppContext } from '../../data/Store'\n\nconst UseContext = (props) => {\n\n    const {state, setState} = useContext(DataContent)\n\n    function addNumber(delta){\n        setState({\n            ...state,\n            number: state.number + delta\n        })\n    }\n\n    const{number,text, setNumber, setText} = useContext(AppContext)\n    useEffect(function(){\n        if(number>1250 ){\n            setText('Eita!')\n        }\n        if(number == 1234){\n            setText('Padrão')\n        }\n        if(number != 1234){\n            setText('Context-API + Hooks')\n        }\n    }, [number])\n\n    return (\n        <div className=\"UseContext\">\n            <PageTitle\n                title=\"Hook UseContext\"\n                subtitle=\"Aceita um objeto de contexto e retorna o valor atual do contexto!\"\n            />\n\n            <SectionTitle title=\"Exercicio #01\" />\n\n            <div className=\"center\">\n\n                <span className=\"text\">{state.text } </span>\n                <span className=\"text\">{state.number } </span>\n            </div>\n\n            <div className=\"center\">\n                <button className=\"btn\" onClick = { () => addNumber(-1)}>\n                    -1\n                </button>\n                <button className=\"btn\" onClick = { () => addNumber(1)}>\n                    +1\n                </button>\n            </div>\n            <SectionTitle title=\"Exercicio #02\" />\n            <div className=\"center\">\n\n                <span className=\"text\">{text}</span>\n                <span className=\"text\">{number}</span>\n                <div>\n                    <button className=\"btn\"\n                    onClick={() =>setNumber(number - 1) } >-1</button>\n                </div>\n                <div>\n                    <button className=\"btn\"\n                    onClick={() =>setNumber(number + 1) } >+1</button>\n                </div>\n\n            </div>\n        </div>\n    )\n}\n\nexport default UseContext\n","C:\\Users\\thiag\\OneDrive\\Área de Trabalho\\cursos\\react\\hooks\\src\\views\\examples\\UseCallback.jsx",[],"C:\\Users\\thiag\\OneDrive\\Área de Trabalho\\cursos\\react\\hooks\\src\\views\\examples\\UseCustom.jsx",[],"C:\\Users\\thiag\\OneDrive\\Área de Trabalho\\cursos\\react\\hooks\\src\\views\\examples\\UseReducer.jsx",[],"C:\\Users\\thiag\\OneDrive\\Área de Trabalho\\cursos\\react\\hooks\\src\\components\\layout\\PageTitle.jsx",[],"C:\\Users\\thiag\\OneDrive\\Área de Trabalho\\cursos\\react\\hooks\\src\\components\\layout\\SectionTitle.jsx",[],"C:\\Users\\thiag\\OneDrive\\Área de Trabalho\\cursos\\react\\hooks\\src\\views\\examples\\UseCallbackButton.jsx",[],"C:\\Users\\thiag\\OneDrive\\Área de Trabalho\\cursos\\react\\hooks\\src\\data\\DataContext.j